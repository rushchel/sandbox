	<xs:complexType name="Extension">
		<xs:annotation>
			<xs:documentation>
				Тип для представления элемента расширения
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the extension element
			</xs:documentation>
		</xs:annotation>
		<xs:anyAttribute namespace="##any" processContents="skip">
			<xs:annotation>
				<xs:documentation>
					Разрешены любые атрибуты для возможности расширения
				</xs:documentation>
