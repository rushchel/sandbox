	<xs:complexType name="CurrencyEncashmentTotal">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий суммы инкассации в одной валюте
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the cash collection amount in the same value
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="CurrencyCode" type="xs:int">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Код валюты
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Currency code
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalCashOutForAllSystem" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Выданные деньги по всем системам
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Money that were dispensed  for all systems
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalCashInForAllSystem" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Полученные деньги по всем системам
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Received money for all systems
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalCashOutForMainSystem" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Выданные деньги по услугам платежной системы
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Dispensed money for all payment system services
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalCashInForMainSystem" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Полученные деньги по услугам платежной системы
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Received money for all paysystem services
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartTotalCashOutput" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма наличности на начало инкассируемого периода на точке
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount of the cash on POP on the next cash collection period start
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EndTotalCashOutput" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма наличности на момент инкассации текущего периода
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount of the cash at the moment of current cash collection period
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartTotalCashInput" type="xs:decimal">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма наличности на начало следующего инкассационного периода. (Сумма наличности после инкасации)
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount of the cash on the next cash collection period start
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="CurrencyEncashmentTotals">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию сумм инкассации в одной валюте
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of the encashment amounts in the same currency
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CurrencyEncashmentTotal" type="CurrencyEncashmentTotal" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Суммы инкассации в одной валюте
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Cash collection amounts in the same currency
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Parameter">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий не типизированный параметр
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the non-type parameter
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" type="Varchar50" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Псевдоним параметра
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Parameter alias
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Значение параметра
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Parameter value
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="Parameters">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию нетипизированных  параметров
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of non-type parameters
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Parameter" type="Parameter" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Параметр
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Parameter
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ParametersQualified">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию не типизированных  параметров
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of non-type parameters
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Parameter" type="Parameter" minOccurs="0" maxOccurs="unbounded" form="qualified">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Параметр
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Parameter
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="InputMethods">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию методов ввода, перечисленных в порядке приоритета.
				Приложение должно выбрать первый тип ввода из поддерживаемых.
				Если ни один метод ввода не поддерживается, то приложение не должно принимать платеж по такой услуге.
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of the data entry methods sorted by priority.
				The application should choose the first supported data entry method.
				If none is supported the application should not accept payments for this service.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="InputMethod" type="InputMethod" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Метод ввода данных
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Data entry method
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="InputMethod">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Метод ввода данных.
				Определяет, каким образом должен быть введен клиентом параметр платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Data entry method. Defines the way for entering the payment parameter.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Attributes" type="Parameters" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Коллекция атрибутов
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Attributes collection
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Alias" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Псевдоним метода ввода
					Поддерживаются следующие методы ввода:
					Keyboard - полноразмерная клавиатура (с переключением языков),
					Keypad - цифровая клавиатура,
					Checkbox - переключатель для ввода значений типа boolean.
					DatePicker - календарь для ввода дат.
					ListBox - выбор из списка допустимых значений.
					Возможные способы ввода могут быть расширены в дальнейшем.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Псевдоним метода ввода
					Поддерживаются следующие методы ввода:
					Keyboard - полноразмерная клавиатура (с переключением языков),
					Keypad - цифровая клавиатура,
					Checkbox - переключатель для ввода значений типа boolean.
					DatePicker - календарь для ввода дат.
					ListBox - выбор из списк адопустимых значений.
					Возможные способы ввода могут быть расширены в дальнейшем.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="PaymentParameter">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
        Информация (метаданные) о параметре платежа.
      </xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing a payment parameter description
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ExtendableObject">
				<xs:sequence>
					<xs:element name="AllowedValue" type="AllowedValue" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								УСТАРЕЛО И ОСТАВЛЕНО ДЛЯ ОБРАТНОЙ СОВМЕСТИМОСТИ.
								Список разрешенных значений для выбора
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								OBSOLETE AND PRESERVED FOR BACKWARD COMPATIBILITY
								The list of the allowed values for selection
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Type" type="PaymentParameterType" form="qualified" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Тип параметра.
								Тип определяет способ вывода параметра на экран.
								Подробнее смотри типы параметров.
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Parameter type
								Parameter type defines the way of parameter displaying.
								See Parameter types for details.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InputMethods" type="InputMethods" minOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Список методов ввода в порядке приоритета использования
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								List of the data entry methods sorted by usage priority
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Name" type="Varchar50" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название, используемое для идентификации параметра платежа.
							Уникально в разрезе каждой услуги (Service).
							Одна услуга НЕ МОЖЕТ иметь несколько параметров платежа с одинаковым названием.
							Пример: "msisdn"
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							The name that is used for payment parameter identifying.
							The name is unique for each Service.
							A service cannot have a few parameters with the same name. Sample "msidn".
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="DisplayName" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название для отображения пользователю при приеме платежа в интерфейсах и для печати в чеке (квитанции).
							Пример: "Номер телефона"
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							The name for displaying in user interfaces and for printing in receipts.
							Sample: "Telephone number"
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Description" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Подсказка пользователю, отображаемая в интерфейсах.
							Пример: "Введите номер телефона 10 цифр без кода страны. Например, 9112001000"
              Может быть пустой строкой.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Prompt dipslayed in UI.
							Sample: "Enter the telephone number without country code. For example, 9112001000"
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Mask" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛО И ОСТАВЛЕНО ДЛЯ ОБРАТНОЙ СОВМЕСТИМОСТИ.
							Содержит значение поля Mask для типа StringPaymentParameterType (описание там же).
							Для других типов содержит пустую строку.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND PRESERVED FOR BACKWARD COMPATIBILITY
							Provides the Mask field value for StringPaymentParameterType type (as well as the description)
							For other types the value is empty string.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Regexp" type="Varchar400" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛО И ОСТАВЛЕНО ДЛЯ ОБРАТНОЙ СОВМЕСТИМОСТИ.
							Содержит значение поля Regexp для типа StringPaymentParameterType (описание там же).
							Для других типов содержит пустую строку.
							В случае пустого значения проверка не производится.
							Регулярное выражение используется ТОЛЬКО для проверки параметров типа строка (element Type - StringPaymentParameterType)
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND PRESERVED FOR BACKWARD COMPATIBILITY
							Provides the Regexp field value for  StringPaymentParameterType type (as well as the description).
							For other types the value is empty string.
							In case of the mepty value the checking is not performed.
							Regular expression us used ONLY for checking the parameters of element Type - StringPaymentParameterType
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="DefaultValue" type="Varchar200" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Значение параметра по умолчанию.
							В случае отсутствия значение по умолчанию не используется.
							Значение по умолчанию присваивается параметру ПЕРЕД отображением пользователю,
							в случае если значение параметра пустое, либо параметра еще нет.
							Т.е. при оплате по шаблону значение по умолчанию не используется, если у параметра уже есть значение.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Default parameter value
							When the paraameter is missing the default value is not used
							Default value is assigned to the parameter BEFORE it is displayed to the user
							in case of empty or missing parameter value
							So, when the payer is used the existing template for entering the data, the default value is not used
							when the parameter is already defined.

						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ErrorMessage" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
              Текст ошибки, который необходимо отображать рядом с полем, в случае, если
              значение параметра не прошло проверку на стороне клиента (длина, ограничения по значению и т. д.).
            </xs:documentation>
						<xs:documentation xml:lang="en-us">

						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="SendAt" type="Requests" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛ И БОЛЕЕ НЕ ИСПОЛЬЗУЕТСЯ. Для совместимости с предыдущими версиями клиентских приложений оставлено значение 3.
							Запросы, в которых должен передаваться данный параметр
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND NO LONGER IN USE. For backward compatibility value 3 is still supported.
							The requests where this parameter should be sent.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Direction" type="ParameterDirection" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Определяет направление передачи параметра. Подробнее смотри документацию к типу.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Defines the parameter transfer direction. See the type description for details.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AvailableValuesReference" type="Varchar50" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛ И БОЛЕЕ НЕ ИСПОЛЬЗУЕТСЯ.
							Название параметра (ссылка на параметр), в котором Сервер возвращает возможные значения этой переменной
							при проверке параметров платежа. В случае наличия этого параметра после проверки параметров платежа
							от пользователя требуется заполнение этого поля.
							Возможные значения передаются в следующем формате:
							[[значение 1]=|[описание для значения 1]|..]
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND NO LONGER IN USE.
							Parameter name (reference) where the server returns the possible values of the variable
							when checking the payment parameters. When the the parameter is defined, the payer should fill in this field
							after parameters checking.
							The available values are transferred as the following:
							[[value 1]=|[description for value 1]|..]
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="IsRequired" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Обязательность параметра.
							true - параметр обязательный, пустые значения недопустимы.
							false - параметр необязательный. Пользователь может не вводить значение параметра.
							При этом его значение приравнивается к пустой строке. Проверка пустого значения по регулярному выражению и ограничениям типа не производится.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Defines whether the parameter is required
							true - required parameter, empty value is not allowed
							false - optional parameter. The parameter may be not defined. In this case it is assumed that
							the parameter value is empty string. No parameter checking by regular expression values and type limitation is performed.

						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="SortOrder" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Порядковый номер.
							Используется для сортировки параметров в интерфейсах и определения порядка печати в чеке.
							В случае, если интерфейс показывает параметры по одному на экран (например, в терминале оплаты), определяет порядк вывода на экран.
							В случае, если интерфейс показывает параметры на одном экране (например, в интернет-банке), определяет порядок сортировки сверху вниз.
							В случае отображения итоговых экранов (подтверждения списка параметров) или печати в чеке, определяет порядок сртировки сверху вниз.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Ordinal number. The attribute is used for sorting the parameters in interfaces and defining the printing order
							in the receipt.
							In case when the parameters are displayed on the screen one by one (e.g. on the terminal),
							the attribute defines the order of displaying.
							In case when all the parameters are displayed in the screen at the same time
							the attributes defines the order of sorting from top to bottom.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PrintToCheque" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Необходимость печати параметра в чеке (квитанции).
							true - необходио распечатать в чеке параметр.
							false - печатать в чеке не требуется.
							При печати параметра типа строка (element Type - StringPaymentParameterType) на параметр накладывается маска (см. Mask).
							При печати параметра типа перечислений (element Type - SupportedValuesPaymentParameterType) на печать выводится не значение параметра, а его отображаемое название.
							При печати остальных параметров выводится их значение как есть.
							Используется только для печати параметров в простых текстовых чеках.
							В случае использования сложных шаблонов для печати (например, приходный кассовый ордер) набор печатаемых параметров определяется шаблоном,
							однако данное поле может быть использовано для вывода параметров в одно из полей такого сложного шаблона (например, в поле Назначение платежа в ПКО).
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Defines whether the parameter should be printed out in the receipt.
							true - it is required to print the parameter in the receipt.
							false -it is not required to prin the parameter in the receipt.
							The parameters of the string type (element Type - StringPaymentParameterType) are printed out masked.
							The parameters of other types are printed out as the are.
							The attribute is only used for printing in simple text receipts.
							In the situations when complex printing templates are used (e.g. cash in order) the set of the printed parameters is defined by template, however
							thid field can be used for parameters output in one of the field of this complex template.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Templated" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Шаблонизируемый
							Параметр определяет, как использовать параметр при оплате по шаблону (или повторе платежа):
							true - значение параметра берется из шаблона (платежа), если оно есть в шаблоне.
							false - значение параметра НЕ берется из шаблона (платежа), даже если оно там есть. Пользователь должен ввести новое значение.
							Рекомендуется в шаблон сохранять все параметры платежа, независимо от значения этого параметра.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Can be reused as a template
							The attribute is defined how to use the parameter when the previous payment is used as a template:
							true - if the value is defined in the template, the parameter value takes the template value
							false - the user need to enter the new value, even the value is defined in the template.
							It is recommended that you save all parameters into template regardless of the attribute value.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Hidden" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Скрытый
							true - параметр скрытый и не должен отображаться пользователю при оплате ни на одном экране.
							Обратите внимание, что скрытый параметр МОЖЕТ печататься в чеке (PrintToCheque).
							false - параметр видим пользователю.
							В случае наличия HiddenRelationParam и HiddenRegexp значение поля вычисляется.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Hidden
							true - the parameter is hidde abd should never be displayed
							Please note that the hidden parameter CAN BE printed (PrintToCheque).
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="HiddenRelationParam" type="Varchar50" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Name параметра платежа, от значения которого зависит необходимость вычисления значения поля Hidden.
							Если поле пустое, то вычисление Hidden НЕ требуется.
							Вычисление выполняется проверкой соответствия регулярному выражению, указанному в поле HiddenRegexp.
							Hidden = true, если значение параметра соответствует (match) регулярному выражению, в противном случае Hidden = false.
							Вычисление необходимо выполнять после того, как пользователь закончит ввод параметра.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							The name of the payment parameter which defined whether the Hidden field value should be calculated.
							if the field is empty, calculation is not required.
							Calculation is performed by checking whether the value matches to the regular expression specified in the HiddenRegexp field.
							Hidden = true, if the parameter value matches the regular expression, otherwise Hidden = false.
							The calculation should be performed when the data entering is completed.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="HiddenRegexp" type="Varchar200" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Регулярное выражение для вычисления значения Hidden. См. HiddenRelationParam.
							В случае, если указана пустая строка, то вычисление НЕ требуется.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Regular expression for entering the Hidden value. See HiddenRelationParam.
							In case when an empty string is specified the claculation IS NOT required.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ReadOnly" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Определяет возможность изменения значения параметра пользователем.
							true - параметр недоступен для изменения.
							false - параметр доступен для изменения.
							Значение параметра НЕ определяет видимость параметра для пользователя. Видимость определяется параметром Hidden.
							В случае наличия ReadOnlyRelationParam и ReadOnlyRegexp значение поля вычисляется.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Defines whether the parameter can be changed by user.
							true - the parameter cannot be changed
							false the parammeter can be changed
							The parameter value DOES NOT define  whether the parameter is hidden, the parameter visibility is defined by the Hidden parameter.
							When ReadOnlyRelationParam and ReadOnlyRegex are specified, the field value is calculated.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ReadOnlyRelationParam" type="Varchar50" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Параметр, от значения которого зависит необходимость вычисления значения поля ReadOnly.
							Если поле пустое, то вычисление ReadOnly НЕ требуется.
							Вычисление выполняется проверкой соответствия регулярному выражению, указанному в поле ReadOnlyRegexp.
							ReadOnly = true, если значение параметра соответствует (match) регулярному выражению, в противном случае ReadOnly = false.
							Вычиление необходимо выполнять после того, как пользователь закончит ввод параметра.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							The name of the payment parameter which defined whether the ReadOnly value should be calculated.
							if the field is empty, calculation IS NOT required.
							Calculation is performed by checking whether the value matches to the regular expression specified in the ReadOnlyRegexp field.
							ReadOnlyRegexp = true, if the parameter value matches the regular expression, otherwise ReadOnlyRegexp = false.
							The calculation should be performed when the data entering is completed.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ReadOnlyRegexp" type="Varchar200" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Регулярное выражение для вычисления значения ReadOnly. См. ReadOnlyRelationParam.
							В случае, если указана пустая строка, то вычисление НЕ требуется.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Regular expression for entering the ReadOnly value. See ReadOnlyRelationParam.
							In case when an empty string is specified the claculation IS NOT required.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="GroupId" type="GUID" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор группы. Отсутствует в случае, если параметр не привязан к группе параметров.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Group identifier. The attribute is missing if the parameter is mpt included in any group.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="State" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Номер состояния, в котором требуется выводить параметр платежа.
							Поле используется только для услуг, набор параметров платежа в которых известен заранее.
							Для услуг, сценарии приема которых управляются с сервера (т.н. многоходовка), поле State не используется.

							Возможны 3 значения состояния:
							1 - Параметры платежа выводятся сразу после запуска сценария платежа.
							После ввода данных всех данных в State=1 необходимо выполнить проверку параметров платежа
							с использованием запроса CheckRequest (или запросов RegisterCheckRequest и GetCheckResultRequest).
							В случае успешной проверки необходимо перейти к отображению параметров из State=2.
							В случае неуспешной проверки необходимо остаться в State=1 и дать возможность пользователю исправить введенные параметры.

							2 - Может отсутствовать. Необходимо отобразить параметры платежа (сервер может вернуть параметры в ответе после State=1),
							после чего перейти к приему наличных (для устройств с приемом наличных) и затем к отправке платежа с использованием
							запроса SendPaymentRequest и проверке статуса платежа с использованием запроса CheckStatusRequest.
							В случае отсутствия параметров после проверки необходимо сразу перейти к отправке платежа.

							3 - Может отсутствовать. В состоянии 3 определены параметры, которые возвращаются хабом ПОСЛЕ отправки платежа поставщику.
							Эти параметры необходимы для печати чека на устройствах самообслуживания, либо для вывода в интерфейс пользователю в других каналах.
							В случае отсутствия хаб не возвращает никаких дополнительных данных после отправки платежа поставщику.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							State number where the payment parameter should be displayed.
							The field is used for the services with the predefined payment parameters set.
							For the services which scenarios are controlled from the server (multi-step scenarios) the State field is not used.
							The following three states may occur:
							1 - Payment parameters are displayed just after running the payment scenario
							After entering all data in State=1 it is required to perform payment parameters checking with RegisterCheckRequest
							and GetCheckResultRequest requests.
							If the checking is successful, jump to State=2 - displaying parameters.
							If checking is not successful, keep State=1 to allow the user to update the wrong parameters.
							2 - May be missing. It is required to display payment parameters
							(the server may return parameters in the response after State=1), then start accepting the cash
							(for the devices equipped with the cash acceptor) and then start the payment sending with  SendPaymentRequest request
							and checking the payment status with CheckStatusRequest request.
							When the payment parameters are missing after the checking, it is required to start  the payment sending.
							3- May be missing. The State=3 defines the parameters which are returned by the server AFTER sending the payment to the provider. These parameters are required for printing the receipt on self-service devices, or for dispalying on the screen when using other channels. When the state is missing the server will not return any additinal data after sending the payment.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="RefreshOnChange" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Требуется или нет обновление состояния при изменении параметра.
							true - при изменении параметра клиентское приложение должно послать запрос обновления (Confirmation = refresh).
							В запросе должны быть указаны текущие значения введенных параметров.
							false или отсутствие - при изменении параметра обновление не требуется.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Determinative" type="xs:boolean" use="optional" default="true">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Параметр ключевой
							true - ключевой, т.е. определяет получателя платежа,
							false - параметр не определяет получателя платежа, а только дополнительные данные по платежу
							По умолчанию выбрано значение true, так как данный параметр влияет на безопасность.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PaymentParameterGroup">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Группа параметров платежа.
				Необходима для группировки параметров, которые нужно выводить пользователю на одном экране в сенсорных устройствах самообслуживания,
				либо объединять в GroupBox в каналах, поддерживающих работу с клавиатурой.
				Может не поддерживаться во фронте.
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Payment parameters group. Required for grouping the parameters that should be displayed at the same time
				in sensor self-service devices, or join into GroupBox in the channels supporting the keyboard.
			</xs:documentation>
		</xs:annotation>
    <xs:complexContent>
      <xs:extension base="ExtendableObject">
        <xs:sequence>
          <xs:element name="InputMethods" type="InputMethods" minOccurs="1">
            <xs:annotation>
              <xs:documentation xml:lang="ru-ru">
                Список методов ввода в порядке приоритета использования
              </xs:documentation>
              <xs:documentation xml:lang="en-us">
                List of the data entry methods sorted by usage priority
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Id" type="GUID" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Идентификатор группы.
              Служит для связи с параметром платежа.
            </xs:documentation>
            <xs:documentation xml:lang="en-us">
              Group identifier.
              Used as a reference to the payment parameter.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DisplayName" type="Varchar50" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Название группы, отображаемое пользователю.
            </xs:documentation>
            <xs:documentation xml:lang="en-us">
              Group name displayed to the user.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Description" type="Varchar200" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Описание группы, отображаемое пользователю в качестве подсказки.
            </xs:documentation>
            <xs:documentation xml:lang="en-us">
              Group description displayed as a prompt.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Format" type="Varchar200" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Строка форматирования группы.
              Служит для вывода параметров, входящих в группу, в одну строку в чеках и на экране пользователю.
              Названия параметров платежа указываются в фигурных скобках {}.
              Например, формат "{secondname} {firstname} {patronymicname}" должен быть преобразован в "Фамилия Имя Отчество".
              Для типа данных строка должна быть использована маска. Для остальных типов формат, если он есть.
            </xs:documentation>
            <xs:documentation xml:lang="en-us">
              String for group formatting.
              The string is intended for displaying the parameters of the same group in the single line on the screen
              and in the receipts.
              The parameter names are displayed in square brackets [].
              For example, the format "{secondname} {firstname} {patronymic}" should be converted to "Second name Name Patronymic".
              Mask should be used for string type data , for other datatypes the format should be used if exists.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="PrintToCheque" type="xs:boolean" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Печатать на чеке.
              true - печатать в чеке.
              false - не печатать в чеке.
              Группа выводится на печать с указанием DisplayName и значения по указанному формату.
              Порядок определяется полем SortOrder.
            </xs:documentation>
            <xs:documentation xml:lang="en-us">
              Printing on the receipt.
              true - print on the receipt
              false - do not print on the receipt
              Group is printed out with specifying the  DisplayName and the value per the specified format.
              The order is defined by the SortOrder field.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SortOrder" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Порядковый номер для сортировки группы и параметров.
              Используется для вывода на печать и на экран.
              Сортировка выполняется от минимального значения к масимальному.
            </xs:documentation>
            <xs:documentation xml:lang="en-us">
              Ordinal number. The attribute is used for sorting the parameters and groups.
              Used for printing out and for displaying on the screen.
              Sorting is performed from minimal value to maximal.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
	</xs:complexType>

	<xs:complexType name="StringPaymentParameterType">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Строковый тип параметра платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Payment value string type
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentParameterType">
				<xs:attribute name="MinLength" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Минимальная длина строки.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Minimal string length.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="MaxLength" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Максимальная длина строки.
							0 - длина не ограничена.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Maximal string length.
							0 - the value is not limited.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Mask" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Маска для ввода значений, упрощающая ввод параметра платежа пользователем.
							Если значение маски - пустая строка, то ввод производится без маски и на устройствах самообслуживания необходимо отображать курсор (без курсора пользователь не увидит вводимые пробелы).

							Формат маски:
							Все символы-заменители должны быть заключены в квадратные скобки [].
							Все символы, которые НЕ требуется вводить, но которые должны стать частью результата ввода, должны быть заключены в фигурные скобки {}.
							Все остальные символы являются литералами, которые должны быть показаны пользователю, но не попадают в результат ввода.
							Внутри квадратных скобок могут быть использованы следующие символы:
							0 - обязательная цифра,
							9 - необязательная цифра,
							A - обязательная буква,
							a - необязательная буква,
							_ - обязательный любой символ,
							- - необязательный любой символ.

							Например,
							маска ввода номера телефона РФ: + 7 ([000]) [000]-[0000] отображается как +7 (***) ***-****. Результат ввода - 10 цифр
							маска ввода номера договора с первой буквой F и 3 или 5 цифрами: {F}[00099] отображается, как F*****. Результат ввода от F000 до F99999.
							маска ввода номера договора от 3 до 5 любых символов: [___--] отображается, как *****.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Mask for simplifying the data entering. If the mask value is empty string, then no mask is used for entering the data, so the cursor should be displayed, otherwise the payer cannot see the spaces on the screen
							Mask format:
							All substitute symbols should be placed in square brackets [].
							All symbols that are NOT required to be entered, but which should be the part of the entered data should be placed in braces {}
							All other symbols are literals, which should be displayed to the user, but are not included in the entered data.
							The following symbols can be used inside the square brackets:
							0 - reguired digit,
							9 - optional digit,
							A - required letter,
							a - optional letter,
							_ - required any character,
							- - optional any character.
							For example:
							The mask for entering the Russian telephone number + 7 ([000]) [000]-[0000] is displayed as: +7 (***) ***-****.  The entered data are ten digits.
							The mask for entering the contract number with the first letter F and with three or five numbers {F}[00099] is displayed as:  F***** . The entered data are the range from F000 to F99999.
							Tha mask for entering the contract number of any three to five characters [___--]  is displayed as *****.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Regexp" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Регулярное выражение для проверки значения параметра ПОСЛЕ ввода.
							В случае ошибки проверки по регулярному выражению результат считается успешным
							(либо ошибка в регулярном выражении, либо платформа не поддерживает такое регулярное выражение).
							В случае пустого значения проверка не производится.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Regular expression for checking the parameter value AFTER entering
							In case of the error when checking by regular exression the result considered successful
							(either the regular expression error, or the platform does not support this regular exression type).
							In case of the empty value, no checking is performed.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DatePaymentParameterType">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Дата.
				Значение передается в формате YYYY-MM-DD. Например, "2015-03-30"
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Date
				Value has format YYYY-MM-DD. For example, "2015-03-30"
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaymentParameterType">
				<xs:attribute name="MinDate" type="xs:dateTime" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Минимальная дата
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Minimal date
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="MaxDate" type="xs:dateTime" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Максимальная дата
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Maximal date
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Format" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Формат вывода.
							d - день месяца от 1 до 31.
							dd - день месяца от 01 до 31.
							ddd - сокращенное название дня недели. Например, ПН.
							dddd - полное название дня недели. Например, понедельник.
							M - месяц от 1 до 12.
							MM - Месяц от 01 до 12.
							MMM - Сокращенное название месяца. Например, окт.
							MMMM - Полное название месяца. Например, октябрь.
							y - год от 0 до 99
							yy - год от 00 до 99
							yyy - год минимум 3 цифры от 001 и выше. Например, 001, 761, 1999, 2014 и т.д.
							yyyy - год 4 цифры. Например, 0001, 2001 и т.д.
							yyyyy - год 5 цифр. Например, 02014.
							\ - escape-символ. Например, \d выводится, как d, а не как 1.
							любой другой символ является литералом, который выводится без изменений.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Output format
							d - day of the month from 1 to 31.
							dd - day of the month from 01 to 31.
							ddd - brief weekday name. For instance, Mo.
							dddd -  full weekday name. For instance, Monday.
							M -month, from 1 to 12.
							MM - month, from 01 to 12.
							MMM - brief month name. For instance Oct.
							MMMM - full month name. For instance , October.
							y - year, from 0 to 99
							yy - year, from 00 to 99
							yyy - year, minimum 3 digits from 001 and higher. For example, 001, 761, 1999, 2014 etc.
							yyyy - year, 4 digits. For example, 0001, 2001 etc.
							yyyyy - year, 5 digits. For example, 02014.
							\ - escape-symbol. For example, \d is dispalyed as d, not as 1.
							any other symbol is the literal that is displayed with no changes.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="BoolPaymentParameterType">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Логическое значение
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Logical value
			</xs:documentation>
		</xs:annotation>
