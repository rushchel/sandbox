	<xs:complexType name="FiscalData">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий информацию о фискальных данных платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the payment fiscal data information
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Extension" type="Extension" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Дополнительные фискальные признаки
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Additional  fiscal parameters
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="FRNumber" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер фискального регистратора
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Number of the fiscal register
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EKLZNumber" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер ЭКЛЗ
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					EKLZ (secure control ribbon) number
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocumentNumber" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер документа по фискальному регистратору
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Document number according to fiscal register
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OperationDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Дата операции
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Operation date
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ShiftNumber" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер смены
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Shift number
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SaleAmountByCash" type="Numeric18.2" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма наличными
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount in cash
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SaleAmountByCard" type="Numeric18.2" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма оплаты безналичными средствами
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount of payments on a cashless basis
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OperationNumber" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер операции по фискальному регистратору
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Operation number according to fiscal register
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="INN" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					ИНН
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					INN (taxpayer ID)
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="PersonalData">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Персональные данные по платежу, связывающие платеж с учетной записью личного кабинета.
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Personal data related to the payment and used for linking the payment to personal cabinet account
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Extension" type="ExtensionItems" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Дополнительные параметры
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Additional parameters
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PersonalAccountId" type="xs:long" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор учетной записи личного кабинета
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Identifier of the personal cabinet account
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TemplateId" type="xs:long" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор шаблона платежа, по которому выполнялся платеж.
					В случае отсутствия платеж не связан с шаблоном.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Identifier of the payment template
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="IdCollection">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий колекцию целочисленных идентификаторов
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of integer identifiers
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Id" type="xs:int" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Целочисленный идентификатор
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Integer identifier
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Invoice">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий карточную операцию
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the card operation
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Extension" type="ExtensionItems" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Дополнительные параметры
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Additional parameters
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PosId" type="Varchar20" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор точки продажи в карточном процессинге.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					POP identifier in card processing
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Id" type="Varchar20" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор операции, отправленной точкой продажи в карточный процессинг.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Identifier of the operation that was sent by POP to card processing
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Time" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Дата и время совершения операции по карте в карточном процессинге.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Date and time of the operation execution according to card processing
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CardNumber" type="Varchar19" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Маскированный номер карты (ISO 7812) длиной до 19 символов.
					Если в запросе отправлен не маскированный номер карты,
					то сервер сделает маскирование символов, кроме первых 6 (BankIdentificationNumber)
					и последних 4, например, 123456******7890.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Masked card number (ISO 7812) up to 19 characters length.
					If  the request includes non-masked card number, the server will mask the characters excluding
					the first 6 ones (BankIdentificationNumber) and the 4 last ones, for example, 123456******7890.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="CardOperationType" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип карточной операции.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Card operation type
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResponseCode" type="Varchar10" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Код ответа карточного процессинга на запрос проведения платежа.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Code of the card processing reply to payment execution request.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Comment" type="Varchar200" use="optional" default="">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Комментарий к коду ответа карточного процессинга на запрос проведения платежа
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Comment to the code of card processing reply to payment execution request
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Success" type="xs:boolean" use="optional" default="true">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
          Успешна ли операция по карте. Сервер сохраняет неуспешные платежи по картам для дальнейшего изучения
          (например, когда точка приема платежа изъяла карту по команде карточного процессинга).
          Платеж по неуспешной операции будет отвергнут Сервером.
        </xs:documentation>
				<xs:documentation xml:lang="en-us">
					Message informing whether the card operation is successful. The server stores the unsuccessful
					card payments for further investigation (for example, when a POP seized the card as was ordered
					by card processing.) The payment for unsuccessful operation will be denied by the server.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ApprovalCode" type="Varchar40" use="optional" default="">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
          Код подтверждения
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
          Approval code
        </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="MultiCurrencyEncashment">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий мультивалютную инкассацию точки
