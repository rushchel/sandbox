			<xs:documentation xml:lang="ru-ru">
				Тип представляющий информацию о номинале
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the denomination information
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="NominalId" type="xs:int">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор номинала
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Denomination identifier
						</xs:documentation>
					</xs:annotation>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CurrencyCode" type="xs:int">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Код валюты
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Currency code
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:decimal">
