				<xs:documentation xml:lang="en-us">
					Legal address
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PostAddress" type="Varchar300" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
          Физический адрес. По умолчанию пустая строка.
        </xs:documentation>
				<xs:documentation xml:lang="en-us">
					Physical address
				</xs:documentation>
			</xs:annotation>
