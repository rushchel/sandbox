						<xs:documentation xml:lang="ru-ru">
							Идентификатор договора
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Contract identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Number" type="Varchar50" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Номер договора
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Contract number
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Date" type="xs:date" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Дата заключения договора
