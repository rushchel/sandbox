	<xs:complexType name="SendPaymentRequest">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий запрос регистрации платежа на сервере
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the request for payment registration on the server
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Request">
				<xs:sequence>
					<xs:element name="PaymentParameters" type="Parameters">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Параметры платежа
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Payment parameters
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Cheques" type="Cheques" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Перечень номеров платежей, сдача с которых использована для оплаты
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Enumeration of the payment numbers for which the change was used for payments
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Invoice" type="Invoice" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Параметры карточной операции. Используется, если платеж был по банковской карте
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Card operation parameters. Are used in case of bank card payments.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FiscalData" type="FiscalData" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Фискальные данные платежа, если платеж был фискализирован
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Payment fiscal data for fiscal payment
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PersonalData" type="PersonalData" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Персональные данные по платежу
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Personal data for payment
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Id" type="GUID" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор платежа. Уникальный среди всех платежей точки.
							Должен совпадать с идентификатором, который отправлялся при проверке параметров платежа.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Identifier of the payment the one unique among all POP payments.
							Should be the same as the identifier that was sent when parameters checking was performed.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Service" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор услуги.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Ticket" type="xs:int" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Номер чека
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Receipt number
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Number" type="Varchar20" use="optional" default="">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Номер платежа, который может быть распечатан в штрих-коде. Рекомендуется составлять его на основе номера точки и времени платежа.
							Например, время платежа 2010-04-10T14:45:34+05:00, номер точки 12345: получаем номер платежа 1234520100410144534
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment number that can be printed out in bar code. It is recommended that it is created using
							the POP number and payment time.
							For example, the payment time is 2010-04-10T14:45:34+05:00,
							the POP number is 12345 : the payment number then is 1234520100410144534
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Time" type="xs:dateTime" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Дата и время оплаты по времени точки.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment date and time according to POP time
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="EncashmentNumber" type="xs:int" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Номер инкассационного периода.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Number of the encashment period
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Value" type="PositiveNumeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сумма платежа к зачислению
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment amount to be deposited
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Commission" type="Numeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Комиссия, взятая с плательщика
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Commission fee for the payer
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Balance" type="Numeric18.2" use="optional" default="0.00">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сдача, которая распечатывается на чеке. Сервер сохраняет информацию о сдаче. Плательщик может потратить её на следующий платеж
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							The cash that is printed out to the receipt. The server stores the change information.
							The payer can use the change for the next payment.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CashOut" type="Numeric18.2" use="optional" default="0.00">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сдача, которая была выдана плательщику наличными средствами
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							The change that was dispensed in cash
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Currency" type="Varchar3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Код валюты операции, в которой совершается платеж. Код определяется стандартом ISO 4217. 3 буквы.
							Пока сервер не поддерживает работы с несколькими валютами, поэтому это поле не проверяется, а валютой платежа является национальная валюта страны, в которой работает Сервер.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Code of the payment currency. The 3-letter code is defined by ISO 4216 standard.
							At this time the server only support operating with single currency, so this field is not checked,
							and the payment currency is considered to be the local currency
							of the area where the server is operating.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="RejectPayment" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Указание серверу не проводить платеж. Истина - платеж не будет проводиться сервером, но будет сохранен, иначе платеж проводится.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Stop-payment-execution directive to the server.
							True - the payment will not be performed, but will be saved.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PaymentTool" type="PaymentTool" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Платежный инструмент
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment tool
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PaymentToolSubType" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Подтип платежного инструмента (ПИ).
							Описание см. в CheckRequest.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Subtype of the payment tool.
							See CheckRequest for description.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Session" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сессия, которая была получена при подтверждении операции в ответе GetNextStateResponse.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CreateTemplate" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							true - сохранить шаблон
							false - не сохранять шаблон
							В случае сохранения шаблона сервер вернет сохраненный шаблон пользователя.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TemplateName" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название сохраняемого шаблона.
							В случае отсутствия или пустой строки название шаблона будет присвоено автоматически.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SendPaymentResponse">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий ответ на запрос регистрации платежа на сервере
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the reply for the request for payment registering on the server
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CheckStatusResponse">
				<xs:sequence>
					<xs:element name="Template" type="PaymentTemplate" minOccurs="0" maxOccurs="1" form="qualified">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Шаблон платежа. Возвращается только в случае, если в запросе был указан CreateTemplate=true.
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Payment template. Renurns in case of request contains CreateTemplate=true.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="CheckStateTimeout" type="xs:duration" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛО И ОСТАВЛЕНО ДЛЯ ОБРАТНОЙ СОВМЕСТИМОСТИ.
							Содержит значение поля NextCheckStateTimeout.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND PRESERVED FOR BACKWARD COMPATIBILITY
							Provides the NextCheckStateTimeout field value
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AvailableBalance" type="NegativeNumeric18.2" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Доступные средства после проведения платежа.
							Отсутствует, если к точке не привязан счет.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Available funds after completing the payment.
							The parameter is not specified when the POP has no linked account.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CurrentBalance" type="NegativeNumeric18.2" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Текущий остаток на счете.
							Отсутствует, если к точке не привязан счет.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Current balance. The parameter is missing if POP has no account.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Currency" type="Varchar3" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Валюта счета.
							Отсутствует, если к точке не привязан счет.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Account currency.
							The parmeter is missing when no account is linked to the POP.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="NeedBlock" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Необходимость прекращения приема платежей, в случае, если заканчиваются средства
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Necessity to stop payments acceptance when the funds are expired
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CreationTemplateCode" type="CreationTemplateResultCode" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Код описывающий результат создания нового шаблона.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Result code describing the result of creation new payment template
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SetPointInfoRequest">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий запрос передачи режима работы точки
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the request for POP working status
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Request">
				<xs:attribute name="FiscalMode" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Признак работы в фискальном режиме
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Indication of working in fiscal mode
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SetPointInfoResponse">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий ответ на запрос передачи режима работы точки
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the reply for POS working mode request
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Response">
				<xs:attribute name="FiscalMode" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Подтверждение режима работы в фискальном режиме
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Confirmation of working in fiscal mode
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GetNextStateRequest">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Запрос получения следующих параметров платежа.
				Используется для проведения операций по услугам, имеющим ProcessPaymentType раным 1.
				Алгоритм:
				1. Пользователь выбирает услугу. Клиентское приложение вызывает GetNextStateRequest.
				2. Сервер возвращает ответ GetNextStateResponse с набором параметров для ввода и набором возможных подтверждений.
				3. Клиентское приложение предлагает клиенту ввести данные формы, после подтверждения ввода отправляет запрос GetNextStateRequest.
				4. Сервер обрабатывает запрос и формирует ответ, как на шаге 2. Процесс повторяется до тех пор,
				пока сервер не вернет ответ с фатальной ошибкой (0), либо с подтверждением успешного окончания сценария (3).
				5. После получения ответа с успешным подтверждением сценария (3) клиентское приложение вызывает SendPaymentRequest.
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Request for receiving the next payment parameters
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Request">
				<xs:sequence>
					<xs:element name="PaymentParameters" type="Parameters" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Параметры платежа, собранные на предыдущем шаге, включая полученные из предыдущего ответа.
								В первом запросе могут отсутствовать. В этом случае сервер вернет первое состояние для ввода параметров.
								В случае, если клиентское приложение использовало справочные данные для построения интерфейса и
								передало в первом запросе параметры на сервер, то сервер сразу вернет следующий шаг.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PersonalData" type="PersonalData" minOccurs="0" maxOccurs="1" form="qualified">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Данные учетной записи личного кабинета, из под которого выполняется операция.
								В случае, если операция выполняется не из личного кабинета, не отправляется ничего.
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Personal data for payment
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="0" maxOccurs="1">
						<xs:element name="ServiceId" form="qualified" type="xs:int" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation xml:lang="ru-ru">
									Идентификатор услуги.
								</xs:documentation>
								<xs:documentation xml:lang="en-us">
									Service identifier
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TemplateId" form="qualified" type="xs:long" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation xml:lang="ru-ru">
									Идентификатор шаблона.
								</xs:documentation>
								<xs:documentation xml:lang="en-us">
									Template identifier
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PaymentSerial" form="qualified" type="xs:long" minOccurs ="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation xml:lang="ru-ru">
									Идентификатор платежа на стороне хаба.
								</xs:documentation>
								<xs:documentation xml:lang="en-us">
									Payment identifier on hub side
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PaymentId" form="qualified" type="GUID" minOccurs ="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation xml:lang="ru-ru">
									Идентифкатор, сформированный точкой для атрибута Id запроса SendPaymentRequest в ранее выполненном платеже, параметры которого должны использоваться для текущего платежа. В этом случае GetNextStateResponse не будет требовать ввода тех параметров платежа, значения которых присутствуют в ранее выполненном платеже.
								</xs:documentation>
								<xs:documentation xml:lang="en-us">
									Payment identifier on client side
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Session" form="qualified" type="xs:string" minOccurs ="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation xml:lang="ru-ru">
									Идентификатор сессии для продолжения оплаты.
								</xs:documentation>
								<xs:documentation xml:lang="en-us">
									Session identifier to continue operation.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="ServiceSerial" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор услуги.
							Устарел и более не используется. См. элемент ServiceId
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service identifier
							Obsolete. Use element ServiceId
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="State" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛ И БОЛЕЕ НЕ ИСПОЛЬЗУЕТСЯ. Сервер должен игнорировать значение этого поля.
							Номер текущего шага.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND NO LONGER IN USE. Server should ignore this field value.
							Current step number
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Language" type="xs:language" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Устарел и оставлен для совместимости. Пeредается в http-header
							Локаль клиента.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Client locale
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Confirmation" type="xs:string" use="optional" default="next">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Результат подтверждения предыдушего состояния. По умолчанию имеет значение next - Далее.
							При первом вызове отправляется значение start.
							В случае, если в списке параметров есть поле с признаком необходимости обновления состояния при изменении,
							то при изменении этого поля в поле Confirmation необходимо указать значение refresh
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Session" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сессия, в рамках которой выполняется взаимодействие.
							При первом запросе не отправляется.
							При последующих запросах отправляется значение, полученное из ответа GetNextStateResponse.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GetNextStateResponse">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий ответ на запрос получения следующих параметров платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the request for receiving the next payment parameters
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Response">
				<xs:sequence>
					<xs:element name="Result" type="GetNextStateResult" minOccurs="1" maxOccurs="1" form="qualified">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Результат запроса
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Request result
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NextStateInfo" type="StateInfo" minOccurs="0" maxOccurs="1" form="qualified">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Информация о шаге ввода атрибутов.
								Присутствует во всех случаях, за исключением фатальной ошибки (ResultCode == 0), когда продолжение сценария не имеет смысла.
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Information about the entering attributes step.
								Exists is all cases excluding the fatal errors when continuing the scenario does not make sense.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaymentParameterValues" type="Parameters" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Параметры платежа.
								Возвращаются только для параметров, определенных в StateInfo.PaymentParameters.
								Данные значения необходимо отправить для подтверждения в SendPaymentRequest.
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Payment parameters
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Session" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сессия, в рамках которой выполняется взаимодействие.
							Возвращается в ответе сервера.
							В случае наличия в ответе это значение должно быть отправлено на сервер в следущем запросе GetNextStateRequest.
							Сессия должна быть отправлена в запросе SendPaymentRequest после завершения сценария.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ServiceId" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор услуги, в рамках которой выполняется сценарий.
							Может измениться в процессе сценария.
							Например, при оплате услуги "Сотовая связь" после определения оператора по номеру телефона
							идентификатор услуги будет изменен на идентификатор услуги оплаты конкретного оператора.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CalculateCommissionRequest">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Запрос расчета комиссии по платежу.
				Запрос используется для расчета комиссии в онлайне на PaySystem Server.
				Запрос может выполняться с одним идентификатором несколько раз, если меняется сумма платежа.
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Request for calculation the commission fee for the payment.
				The request is used for calculation the commission fee in online mode on the PaySystem Server.
				The request can be executed repeatedly with the same ID if the payment amount is changed.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Request">
				<xs:sequence>
					<xs:element name="PaymentParameters" type="Parameters">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Параметры платежа
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Payment parameters
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PersonalData" type="PersonalData" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Персональные данные по платежу
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Personal data for payment
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Id" type="GUID" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор платежа. Уникальный среди всех платежей точки.
							Должен совпадать с идентификатором, который отправлялся при проверке параметров платежа.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Identifier of the payment the one unique among all POP payments.
							Should be the same as the identifier that was sent when parameters checking was performed.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Service" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор услуги.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Time" type="xs:dateTime" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Дата и время платежа по времени точки.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment date and time according to POP time
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Amount" type="PositiveNumeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сумма платежа к зачислению
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment amount to be deposited
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AmountType" type="AmountType" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Признак того какая сумма известна (Total или Value). Комиссия всегда рассчитывается от суммы перевода, которая будет перечислена поставщику услуги.
							Если указано 0 (значение по умолчанию) - используется прямая формула расчета. Если указано 1 (Total) - обратная формула
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Indication whether Total or Value amount is known. The commission fee is always calculated from the payment amount that will be transferred to the service provider.
							If 0 specified (default value) the direct formula is used.
							If 1 (Total) is specified the backward formula is used.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Currency" type="Varchar3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Цифровой код валюты операции
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Digital code of the operation currency
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PaymentTool" type="PaymentTool" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Платежный инструмент
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment tool
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PaymentToolSubType" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Подтип платежного инструмента (ПИ).
							Описание см. в CheckRequest.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Subtype of the payment tool.
							See CheckRequest for description.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CalculateCommissionResponse">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Ответ на запрос расчета комиссии по платежу.
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Reply for the request for calculation the commission fee on the payment
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Response">
				<xs:sequence>
					<xs:element name="CommissionInfo" type="CommissionInfo" minOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Сведения о комиссии, удерживаемой с клиента
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Information about the commission fee that the client should be charged for
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Id" type="GUID" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор платежа такой же, как в запросе
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Payment identifier same as in the request
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Commission" type="Numeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Общая сумма комиссии, которую нужно удержать с клиента
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Total commission fee that the client should be charged for
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Value" type="Numeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сумма перевода, которая будет перечислена поставщику.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Total amount that will be transferred to the provider
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Total" type="Numeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Сумма перевода + общая сумма комиссии
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Total amount plus total commission fee
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

