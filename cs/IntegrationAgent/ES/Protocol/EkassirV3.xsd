	<xs:complexType name="ServerPayment">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий информацию о платеже
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the payment information
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PayParameters" type="Parameters"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Коллекция параметров платежа
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Collection of payment parameters
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Service" type="Service" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Достаточная информация о сервисе, на который был сделан платеж
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Sufficient information about the service that was paid
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Point" type="Point" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Информация о точке, на которой был сделан платеж
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Information about the POP where the payment was made
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FiscalData" type="FiscalData" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Информация о фискальной операции, если платеж был фискализирован
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Information about the fiscal operation in case of the fiscal payment
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Client" type="Client" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Информация о клиенте, через которого прошел платеж
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Information about the client involved in the payment
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contract" type="Contract" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Договор клиента, через который был проведен платеж.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Client contract  involved in the payment
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operator" type="Operator" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Оператор, через которого был проведен платеж
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Operator involved in the payment
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Cheques" type="Cheques" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Коллекция чеков, которые были использованы  в платеже
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Collection of the receipts that were used in the payment
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Invoice" type="Invoice" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Информация о карточной операции, если платеж был совершен с помощью карты
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Information about card operation if the payment was performed with the card
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Serial" type="xs:long" minOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Номер транзакции платежного сервера
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Transaction number on the payment server
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Id" type="GUID" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор платежа
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment identifier
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Ticket" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер чека на кассе
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Receipt number in cash-machine
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Number" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер платежа
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment number
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ClientTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Время совершения платежа на точке
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Time of payment execution on the POP
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ServerTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Время прихода платежа на сервер
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Time of payment arrival to the server
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EncashmentNumber" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Номер инкассационного периода, в котором был совершен платеж
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Number of encashment period when the the payment was made
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма к зачислению
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount to be deposited
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Commission" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Комиссия взятая с плательщика
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Comission fee to be paid by a payer
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Balance" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма сдачи образовавшаяся на чеке
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount of the change placed on the receipt
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CashOut" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма сдачи выданная наличными средствами
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount of money dispensed as a change
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Amount" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма списания со счета
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount to be withdrawed from the account
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FromBalance" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма чеков использованных в платеже
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Amount on receipts that were used in the payment
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentState" type="PaymentState" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Статус платежа
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment status
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="FullService">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий полную информацию об услуге
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing full information about the service
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Service">
				<xs:sequence>
					<xs:element name="Requisites" type="Requisites">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Реквизиты поставщика услуги
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Service provider details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaymentParameters" type="PaymentParameters" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Список описания параметров платежа, необходимых для принятия платежа по этой услуге
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								List of the payment parameters required for payment acceptance for this
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaymentParametersGroups" type="PaymentParameterGroups" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Список описания групп параметров платежа
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								List of the payment parameters group descriptions
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CommissionLimitations" type="CommissionLimitations" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Список ограничений на размер комиссии по услуге
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								List of the limitations for the service comission
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="MinValue" type="Numeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Минимальная сумма платежа по услуге
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Minimal payment amount for this service
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="MaxValue" type="Numeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Максимальная сумма платежа по услуге
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Maximum payment for this service
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Currency" type="Varchar3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Валюта, в которой выражены минимальная и максимальная сумма платежа
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Currency  for minimal and maximal payment amount
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CheckNecessity" type="CheckNecessity" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Необходимость проверки параметров платежа перед отправкой платежа
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Necessity to check payment parameters before sending
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CheckTimeout" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Рекомендуемый таймаут проверки параметров платежа в секундах
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Recommended timeout for payment parameters checking in seconds
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AcceptUnknownParameters" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Признак сохранения неизвествных параметров в платеже
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Indication of saving the unknown parameters in the payment
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ProcessPaymentType" type="ProcessPaymentType" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Тип схемы проведения платежа.
							В случае отсутствия поля значение равно 0.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Type of the payment execution scheme
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="OnlineProcessing" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Признак обработки операции online.
							false - Платежи по услуге обрабатываются асинхронно: при получении запроса SendPaymentRequest
							система сохраняет платеж в БД возвращает ответ SendPaymentResponse,
							после чего выполняет отправку получателю. Вызывающая сторона должна выполнять проверку статуса
							CheckStatusRequest.
							true - Платежи по услуге обрабатываются по возможности синхронно: при получении запроса SendPaymentRequest
							система сохраняет платеж в БД, затем пытается обработать платеж. В случае, если платеж обработан за время
							OnlineProcessingTimeout, то вызывающая сторона получит ответ с результатом обработки.
							В случае, если время обработки платежа заняло больше времени, чем требуется для ответа,
							система вернет результат на момент истечения OnlineProcessingTimeout и продолжит обработку в асинхронном режиме.
							В случае отсутствия атрибута значение = false.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="OnlineProcessingTimeout" type="xs:duration" use="optional" default="PT30S">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Время в секундах, которое требуется для передачи ответа вызывающей стороне о результах обработки платежа.
							В случае, если обработка платежа потребовала больше времени, чем значение таймаута,
							то сервер дает ответ с текущим статусом обработки платежа и продолжает обработку.
							В этом случае вызывающая сторона получит не финальный статус платежа, а промежуточный, поэтому потребуется
							выполнять проверку статуса с помощью CheckStatusRequest.
							В случае отсутствия атрибута значение = 30.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="IsTemplated" type="xs:boolean" use="optional" default="true">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Возможность сохранения шаблона платежа по услуге.
							true - шаблон сохранять можно.
							false - шаблон сохранять нельзя.
							В случае отсутствия - true.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Cts" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Символ кассового плана услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Сash plan symbol for service
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>

			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="FullServices">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию услуг с полной информацией о них
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of the services with full information
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Service" type="FullService" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Полная информация об услуге
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Full service information
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Service">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий достаточную информацию об услуге
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the sufficient informaiton for the service
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ShortService">
				<xs:sequence>
					<xs:element name="RegionIds" type="IdCollection" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Список идентификаторов регионов, в которых действует поставщик услуги.
								Позволяет реализовать фильтрацию услуг по регионам в зависимости от нахождения клиента или устройства самообслуживания.
								Для обратной совместимости новых версий клиентов со старой версией сервера
								в случае отсутствия привязки услуги к регионам элемент не возвращается (имитируется поведение предыдущей версии сервера).
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								List of the regions identifiers covered by the service provider.
								Allows you to filter the services by regions depending on the client or self-service device location.
								For backward compatibility of the new client versions with the old server version.
								If the service is not linked to the region, the parameter is not returned (behavior of the previous server version.)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Name" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service name
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TypeId" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор типа услуги.
							В случае, если привязки к типу услуги нет, то атрибут отствует.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service type identifier
							If no link to service type, the parameter is missing.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="RegionId" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							УСТАРЕЛ И ОСТАВЛЕН ДЛЯ ОБРАТНОЙ СОВМЕСТИМОСТИ.
							См. RegionIds
							Идентификатор региона услуги.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							OBSOLETE AND USED FOR BACKWARD COMPATIBILITY.
							see RegionIds
							Service region identifier.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Logo" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название файла логотипа услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service logotype file name
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Alias" type="Varchar20" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Псевдоним услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service alias
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="DefaultOperatorId" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Оператор по умолчанию
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Default operator
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Class" type="Varchar20" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Класс услуги.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service class
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="MediumServices">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию услуг с достаточной информацией о них
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of the services with sufficient information
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Service" type="Service" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Достаточная информация об услуге
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Sufficient information about the service
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ServiceType">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий тип услуги
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the service type
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ExtendableObject">
				<xs:attribute name="Id" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор типа услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service type identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Name" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Имя типа услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service type name
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ParentId" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор родительского типа услуги.
							В случае, если родителя нет, то атрибут отсутствует.
							Служит для выстраивания дерева типов услуг.
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Identifier of the parental region. If no parental region, the attribute is missing.
							This attribute allows you to create the region tree.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Logo" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Логотип
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Logotype
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ServiceTypes">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию типов услуг
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the service types collection
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ServiceType" type="ServiceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Тип услуги
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Service type
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ShortService">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий минимальную информацию об услуге
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing minimal information about the service
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ExtendableObject">
				<xs:attribute name="Id" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Version" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Версия услуги
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service version
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ExternalId" type="Varchar100" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор услуги во внешней учетной системе
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Service identifier in the external system
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ShortServices">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию услуг с минимальной информацией о них
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the collection of the services with minimal information
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Service" type="ShortService" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Минимальная информация об услуге
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Minimal information about the service
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ServiceClass">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий информацию о классе услуги
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Represents information about service class
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Alias" type="Varchar20" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Алиас класса
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Alias of the service class
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DisplayName" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Отображаемое имя класса
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Display name
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OperationType" type="ServiceOperationType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип операции
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Operation type
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="ServiceClassList">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию классов услуг
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Represents a collection of the service classies
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ServiceClass" type="ServiceClass" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Информация о классе услуги.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Information about service class
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CommissionLimitation">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий правило ограничения комиссии
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Тип представляющий правило ограничения комиссии
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="StartDate" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Дата, с которой действует данное ограничение.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Limitation start date
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" type="LimitationType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип ограничения
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Limitation type
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MinRate" type="Numeric6.3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Минимальное ограничение комиссии в процентах.
					0, если нет ограничения по параметру.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Minimal commission amount in monetary units, 0 - if no limitation
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaxRate" type="Numeric6.3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Максимальное ограничение комиссии в процентах.
					0, если нет ограничения по параметру.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Maximum commission limitation in percent, 0 if no commission
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MinAmount" type="Numeric18.2" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Минимальная сумма комиссии в денежных единицах.
					0, если нет ограничения по параметру.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Minimal commission amount in monetary units, 0 - if no limitation
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaxAmount" type="Numeric18.2" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Максимальная сумма комиссии в денежных единицах.
					0, если нет ограничения по параметру.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Maximal commission amount in monetary units, 0 - if no limitation
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CommissionType" type="CommissionType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип комиссии
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Commission type
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="CommissionLimitations">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий коллекцию правил ограничения комиссии
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the commission limitation rules collection
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CommissionLimitation" type="CommissionLimitation" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Правило ограничения комиссии
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Commission limitation rule
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="AmountType">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип суммы, переданной для вычисления комиссии
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type of the amount for commission calculation
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Сумма перевода (Value)
						Сервер рассчитает от нее сумму комиссии (Commission).
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Transferring amount (Value).
						The server calculates the commission fee amount (Commission).
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Сумма (Total), включающая сумму перевода (Value) и сумму комиссии (Commission).
						Сервер рассчитает сумму перевода (Value) и сумму комиссии (Commission) по обратной формуле.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Amount (Total) that includes the transferring amount (Value) and the commission fee amount (Commission).
						The server calculates the transferring amount (Value) and the commission fee amount (Commission)
						by backward calculation.
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="CommissionInfoList">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Список сведений о комиссии
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				List of the commission details
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CommissionInfo" type="CommissionInfo" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CommissionInfo">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Сведения о комиссии
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Commission details
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CalculationRuleList" type="CalculationRuleList" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="Version" type="xs:long" use ="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Версия.
					Используется при загрузке справочника комиссий.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Version. Used when the commission directory is loaded.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ServiceType" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор типа услуги
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Service type identifier
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Service" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор услуги
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Service identifier
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentTool" type="PaymentTool" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип платежного инструмента.
					В случае, если не указан тип ПИ, то комиссия действует для всех видов ПИ, кроме тех, для которых комиссия указана явно.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment tool type.
					If not specified the commission works for all payment tools except the ones
					where the commission is specified explicitly.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentToolSubType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Подтип платежного инструмента (ПИ).
					В случае, если не указан подтип ПИ, то комиссия действует для всех подтипов в рамках данного типа, за исключением тех, для которых комиссия не указана явно.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment tool subtype.
					If not specified the commission works for all payment subtypes for this tools except the ones
					where the commission is specified explicitly.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use ="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Описание комиссии для клиента
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Description of the commission for the client
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="AvailableServiceInfoList">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Список сведений о доступности услуг
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				List of the available services
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AvailableServiceInfo" type="AvailableServiceInfo" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AvailableServiceInfo">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Сведения о доступности услуги для конкретного платежного инструмента
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Information about availability service for specified payment tool.
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Service" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор доступной услуги.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Identifier of available service
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentTool" type="PaymentTool" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип платежного инструмента для которого доступна данная услуга
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment tool for which the current service is available
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentToolSubType" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Подтип платежного инструмента для которого доступна данная услуга
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Payment tool subtype for which the current service is available
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="CalculationRuleList">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Список формул
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				List of the commission rules
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Rule" type="CalculationRule" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CalculationRule">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Формула для расчета комиссии от суммы перевода (Value).
				Constant + Percent * Value / 100
				Действует для диапазона сумм переводов StartValue - EndValue и в течение времени суток StartTime - EndTime
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Formula for calculating the commission fee for the payment amount (Value)
				Constant + Percent * Value / 100
				To be applied to StartValue - EndValue range of the payment amounts during StartTime - EndTime time period
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Constant" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Постоянная составляющая.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Constant part
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Percent" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Процент
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Percent
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartValue" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Начало интервала с которого действует данная формула (Включительно)
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Beginning of the payment amount interval for applying the formula (including the startpoint)
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EndValue" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Конец интервала до которого действует данная формула (НЕ включительно)
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					End of the payment amount interval for applying the formula (excluding the endpoint)
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartTime" type="xs:time" use ="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Начало временно интервала, с которого действует данная формула (Включительно)
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Beginning of the time period interval for applying the formula (including the startpoint)
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EndTime" type="xs:time" use ="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Конец временно интервала, до которого действует данная формула (НЕ включительно)
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					End of the time period interval for applying the formula (excluding the endpoint)
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="PaymentToolSubTypes">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Список подтипов платежных инчтрументов
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				List of the payment subtypes
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentToolSubType" type="PaymentToolSubType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PaymentToolSubType">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Подтип платежного инструмента
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Payment tool subtype
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Alias" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Алиас
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Alias
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IsDefault" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Определяет, является ли по умолчанию данный подтип для типа платежного инструмента, инструментом по умолчанию
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Defines whether this subtype is the default type of this payment type
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name ="PaymentTool" type="PaymentTool" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип платежного инструмента, к которому от носится данный под тип
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Parent payment tool type for this subtype
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="AvailablePaymentTool">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Допустимый к использованию платежный инструмент
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Allowed payment tool
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name ="PaymentTool" type="PaymentTool" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Тип разрешенного платежного инструмента
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Allowed payment tool type
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentToolSubType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Подтип разрешенного платежного инструмента.
					В случае отсутствия разрешены все подтипы платежных инструменов.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Subtype of the allowed payment tool
					When this parameter is missing all payment tools subtypes are allowed
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="AvailablePaymentToolList">
		<xs:sequence>
			<xs:element name ="AvailablePaymentTool" type="AvailablePaymentTool" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="StateInfo">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Информацию о следующем шаге проведения платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Information about the next payment step
			</xs:documentation>
		</xs:annotation>
    <xs:complexContent>
      <xs:extension base="ExtendableObject">
		<xs:sequence>
			<xs:element name="PaymentParameters" type="NextStatePaymentParameters" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Список описания параметров платежа, необходимых для принятия платежа по этой услуге
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						List of the payment parameters required for payment acceptance for this service
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentParametersGroups" type="NextStatePaymentParameterGroups" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Список описания групп параметров платежа.
						В случае отсутствия групп список не будет возвращен.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						List of the parameter group descriptions
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConfirmationList" type="ConfirmationList" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Список подтверждений состояния.
						В случае отсутствия списка в ответе проверка параметров должна быть прекращена.
						В случае присутствия списка в ответе, допустимы те подтверждения, которые перечислены в списке.
						Отмена операции возможна в клиентском приложении независимо от наличия или отсутствия cancel.
						В случае, если cancel отсутствует в ответе, то при отмене операции отправлять запрос с Confirmation = cancel не требуется.
						В случае, если cancel в ответе есть, то при отмене необходимо отправить запрос с Confirmation = cancel на сервер.
						Важно: может отстутствовать подтверждение next (Далее).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="State" type="xs:int" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					УСТАРЕЛ И ОСТАВЛЕН ДЛЯ ОБРАТНОЙ СОВМЕСТИМОСТИ.
					Номер шага.
					Возвращается только для клиентских приложений, работающих по версиям протокола 3.14 и ниже.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Step number
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StateDisplayName" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Название шага
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Step name
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StateDescription" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Описание шага
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Step description
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
        </xs:extension>
      </xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Confirmation">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Подтверждение состояния
			</xs:documentation>
		</xs:annotation>
    <xs:complexContent>
      <xs:extension base="ExtendableObject">
        <xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Название подтверждения, которое используется для отправки на сервер.
					Стандартные названия:
					next - далее,
					back - назад,
					cancel - отмена.
					АТМ и терминалы поддерживают только стандартные подтверждения.
					Web и мобильные могут поддерживать дополнительные.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DisplayName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Текст подтверждения, который выводится пользователю.
					Для подтверждений next, cancel, back текст может отсутствовать.
					В этом случае клиентское приложение берет стандартный текст для подтверждения.
					Дополнительные подтверждения должны иметь текст.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Image" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Название изображения без расширения, используемого в качестве кнопки подтверждения.
					Используется для тех клиентских приложений, где в качестве кнопок используются изображения.
					По умолчанию отсутствует.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
      </xs:extension>
    </xs:complexContent>
	</xs:complexType>
  <xs:complexType name="ConfirmationInfo">
    <xs:annotation>
      <xs:documentation xml:lang="ru-ru">
        Подтверждение состояния
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Confirmation">
        <xs:attribute name="Id" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Идентификатор подтверждения дейтсвия
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="IsDefault" type="xs:boolean" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Признак что подтверждение является дефолтным.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="State" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="ru-ru">
              Номер шага для которого используется.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
	<xs:complexType name="ConfirmationList">
		<xs:sequence>
			<xs:element name="Confirmation" type="Confirmation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
  <xs:complexType name="ConfirmationInfoList">
    <xs:sequence>
      <xs:element name="ConfirmationInfo" type="ConfirmationInfo" minOccurs="0" maxOccurs="unbounded" form="qualified"/>
    </xs:sequence>
  </xs:complexType>
	<xs:complexType name="GetNextStateResult">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Результат запроса предыдущего шага
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type representing the information about the result of the next step request
			</xs:documentation>
		</xs:annotation>
    <xs:complexContent>
      <xs:extension base="ExtendableObject">
		<xs:sequence>
			<xs:element name="Errors" type="ValidationErrors">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Список ошибок проверки параметров платежа
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						List of the errors of the payment parameters checking
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ResultCode" type="GetNextStateResultCode" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Код результата обработки запроса.
					0 - окончание сценария по ошибке. Сообщение пользователю необязательное и указано в поле Message.
					1 - ошибка в параметре. Текст сообщения может быть привязан к параметру, либо может быть общее сообщение об ошибке в Message.
					2 - продолжение обработки операции. GetNextStateResponse содержит очередной набор параметров платежа, значения которых необходимо передать в очередном запросе GetNextStateRequest.
					3 - операция завершена. Вызовы GetNextStateRequest более не требуются.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Code of the request processing result
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Message" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Общее сообщение об ошибке. Может быть пустой строкой.
					В случае, если сообщение об ошибке - пустая строка и ResultCode равен 0, то показывать ошибку пользователю не требуется.
					Нужно выйти из сценария и прекратить взаимодейcтвие с пользователем.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					General error message
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
      </xs:extension>    
    </xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ValidationErrors">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Коллекция ошибок проверки параметров платежей
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Collection of the checking payment parameters errors
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Error" type="ValidationError" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Информация об ошибке параметра платежа
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Information about the payment parameter error
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ValidationError">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Информация об ошибке при проверке параметра платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Information about hte error when payment parameters checking
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="PaymentParameterName" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Имя ошибочного параметра платежа
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					The name of the wrong payment parameter name
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Описание ошибки
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Error description
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="PaymentTemplate">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Информация о параметрах добавляемого шаблона платежа
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Parameters" type="ParametersQualified" minOccurs="0" maxOccurs="1" form="qualified">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Параметры платежа, которые отображаются клиенту при просмотре шаблона.
						Отсутствует в случае запроса списков.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Payment parameters visible to client.
						Empty in case of templates list request.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentParameterDescriptors" type="PaymentParameters" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Список описаний параметров платежа, отображаемых клиенту.
						Отсутствует в случае запроса списков.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						List of the payment parameters descriptiors visible to client.
						Empty in case of templates list request.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PersonalData" type="PersonalData" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Персональные данные.
						Могут отсутствовать.
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Personal data.
						Могут отсутствовать.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Extension" type="ExtensionItems" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Дополнительные параметры
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Additional parameters
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Id" type="xs:long" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор шаблона платежа
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ServiceId" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор услуги
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="PaymentTemplateAttributes"/>
		<xs:attribute name="CreationTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Дата и время создания шаблона платежа
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastUsageTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Дата и время последнего использования шаблона платежа.
					Обновляется при каждом использовании шаблона для оплаты.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastPaymentAmount" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма последнего платежа по шаблону без комиссии.
					Обновляется каждый раз при оплате по шаблону.
					В случае отсутствия платежей по шаблону поле отсутствует.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastPaymentCurrency" type="Varchar3" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Символьный код валюты последнего платежа по шаблону согласно стандарту ISO 4712
					В случае отсутствия платеже по шаблону поле отсутствует.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LastPaymentId" type="xs:long" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Идентификатор последнего платежа по шаблону.
					В случае отсутствия платежей по шаблону поле отсутствует.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="UsageCount" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Количество использований шаблона для оплаты.
					Обновляется каждый раз при оплате.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ServiceVersion" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Версия услуги, для которой был создан шаблон.
					Заполняется автоматически при оплате услуги по шаблону.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="UpdateTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang ="ru-ru">
					Дата и время обновления шаблона
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Deleted" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Признак того, что шаблон был удален пользователем:
					true - удален,
					false - не удален.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:anyAttribute namespace="##any" processContents="skip">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					В базовом типе запроса разрешено добавление любых атрибутов.
					Это необходимо для добавления новых параметров в уже существующие запросы с сохранением обратной совместимости с предыдущими версиями протокола.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					The base request type allows adding any attributes.
					This is required when adding the new parameters in the existing requests in order to preserve backward compatibility with the previous protocol versions.
				</xs:documentation>
			</xs:annotation>
		</xs:anyAttribute>
	</xs:complexType>

	<xs:attributeGroup name="PaymentTemplateAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Группа атрибутов шаблона платежа, которая используется для создания и обновления шаблона платежа
				Создана для оптимизации схемы
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Название шаблона платежа
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentAmount" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Сумма платежа по шаблону без комиссии.
					Может использоваться фронтом для предзаполнения суммы при оплате, либо для оплаты с использованием USSD-команд и т.д.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaymentCurrency" type="Varchar3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Символьный код валюты платежа по шаблону согласно стандарту ISO 4712.
					См. описание PaymentAmount
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IsFavorite" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Признак любимого шаблона:
					true - шаблон любимый,
					false - шаблон обычный.
					В случае отсутствия поля значение = false.
				</xs:documentation>
        <xs:documentation xml:lang="en-us">
          Sign of favorite template:
          true - favorite template,
          false - general template.
          In case of absence of field value = false.
        </xs:documentation>
      </xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>

	<xs:complexType name="PaymentTemplateList">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Список шаблонов платежей
			</xs:documentation>
		</xs:annotation>
