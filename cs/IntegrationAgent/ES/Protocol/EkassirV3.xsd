  <!--Сложные типы-->
	<!--Complex types-->

	<xs:complexType name="ExtendableObject">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Базовый тип для возможности поддержки динамических свойств объектов
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Base type for supporting the dynamic properties of objects
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Parameters" type="Parameters" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Дополнительные параметры
					</xs:documentation>
					<xs:documentation xml:lang="en-us">
						Additional parameters
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:anyAttribute namespace="##any" processContents="skip">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Объект может содержать любой атрибут. Это позволяет расширять объекты в следующих версиях схемы с сохранением обратной совместимости.
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					An object can include any attribute. This allows you to extend objects in future
					schema versions and prevent backward compatibility.
				</xs:documentation>
			</xs:annotation>
		</xs:anyAttribute>
	</xs:complexType>

	<xs:complexType name="Error">
		<xs:annotation>
			<xs:documentation>
				Тип для представления ошибки
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing an error
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Number" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Код ошибки:
					1	- Неправильный запрос (сервер не смог разобрать данные запроса)
					2	- При обработке запроса произошла внутренняя ошибка сервера
					3	- Услуга недоступна
					4	- Клиент (или точка) заблокирован
					5	- Отсутствуют необходимые параметры платежа
					6	- Недостаточно средств
					7	- Такой платеж уже есть
					8	- Отмена невозможна
					9	- Отмена временно не возможна
					10	- Платеж не найден
					11	- Отмена невозможна, так как платеж отвергнут (нет смысла его отменять)
					12	- Шаблон не найден
					Код ошибки не определен схемой, по причине возможности его расширения
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Error code:
					1	- Bad request (server was not able to recognize the request data)
					2	- Internal server error when processing the request
					3	- Service is not available
					4	- Client (or POP) is blocked
					5	- The required payment parameters are missing
					6	- Unsufficient funds
					7	- The payment already exists
					8	- Cancelling is unavailable
					9	- Cancelling is temporary unavailable
					10	- The payment is not found
					11	- Cancelling is unavailable as the payment was denied (cancelling does not make sense).
					12	- The template is not found
					The error code is not defined by schema, as it may be extended.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Текстовое описание ошибки, в данной версии не локализуется
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Error text description (not localizedin the current version)
				</xs:documentation>

			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IsFatal" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>
					Признак фатальности ошибки.
					Если ошибка фатальна, то повторные запросы с такими же параметрами платежа не имеют никакого смысла
					и приведут к тому же результату. Если ошибка не фатальна, то повтор запроса с теми же параметрами платежа
				</xs:documentation>
				<xs:documentation xml:lang="en-us">
					Indication of fatal error. If the error is fatal, the repeated requests do not make sense.
					If the error is not fatal, repeat the request with the same parameters.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="AuthorizationSchema">
		<xs:sequence>
			<xs:element name="Parameters" type="Parameters" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Набор параметров схемы авторизации.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ru-ru">
					Название схемы авторизации. Например, "otp".
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="AuthorizationSchemaList">
		<xs:sequence>
			<xs:element name="AuthorizationSchema" type="AuthorizationSchema" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="ru-ru">
						Список схем авторизации.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

