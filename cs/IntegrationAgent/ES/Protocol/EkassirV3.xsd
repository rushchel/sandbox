	<xs:complexType name="Account" >
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий информацию об учетном счете клиента или точки
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the information about the  client or POP account
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ExtendableObject">
				<xs:attribute name="Id" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор счета
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Account identifier
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Name" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название счета
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Account name
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AllowedBalance" type="NegativeNumeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Разрешенный лимит точки (минимально возможная сумма на счете)
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Allowed POP limit (minimal amount on the account)
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CurrentBalance" type="NegativeNumeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Текущие средства на счете
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Current balance of the account
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AvailableBalance" type="NegativeNumeric18.2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Доступные средства
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Available funds
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Currency" type="Varchar3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
              Трех-буквенный код валюты счета по ISO-4712.
            </xs:documentation>
						<xs:documentation xml:lang="en-us">
							Account currency
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="RealAccount" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Банковский номер счета
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Account number
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Operator">
		<xs:annotation>
			<xs:documentation xml:lang="ru-ru">
				Тип представляющий оператора платежа
			</xs:documentation>
			<xs:documentation xml:lang="en-us">
				Type for representing the payment operator
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ExtendableObject">
				<xs:sequence>
					<xs:element name="Requisites" type="Requisites">
						<xs:annotation>
							<xs:documentation xml:lang="ru-ru">
								Реквизиты оператора
							</xs:documentation>
							<xs:documentation xml:lang="en-us">
								Operator details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Id" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор оператора
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Идентификатор оператора
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Name" type="Varchar200" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Название оператора
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Operator name
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ContractId" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="ru-ru">
							Идентификатор договора с этим оператором
						</xs:documentation>
						<xs:documentation xml:lang="en-us">
							Identifier of the contract with this operator
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Operators">
